<div ng-controller = "addProjectCtrl">

<div class="hold-transition skin-blue sidebar-mini">
  <div>
    <section class="content">
	<div class="row">
		<div class="col-lg-5 col-xs-5">
		</div>
		<div class="col-lg-4 col-xs-4">
			<h1> ADD PROJECTS </h1>
			<br>
		</div>
		<div class="col-lg-4 col-xs-4">
		</div>
	</div>
	<form>
      <div class="row ">
	  <section>
	  <div class="col-lg-3 col-xs-3">
	  </div>
	  		<div class="col-lg-2 col-xs-2">
			<label>Project Name</label>
		</div>
		<div class="col-lg-4 col-xs-4">
			<input class="form-control" type="text" id="projectName" name="projectName" placeholder="Project Name" ng-model="projectName" required />
		</div>
		</section>
		</div>

	    <hr/>
		<div class="row">
		<div class="col-lg-3 col-xs-3">
	  </div>
		<div class="col-lg-2 col-xs-1">
			<label>Project Description</label>
		</div>
		<div class="col-lg-4 col-xs-4">
			<input class="form-control" type="text" id="description" name="description" placeholder="Description" ng-model="description" required />
		</div>
		</div>

		 <hr/>

	<div class="row">
		<div class="col-lg-3 col-xs-3">
		</div>
		<div class="col-lg-2 col-xs-2">
			<label>Type of Testing</label>
		</div>
		<div class="col-lg-4 col-xs-4">

			<select name="testingType" id="testingType" ng-model="testingType" required class="form-control">
				<option value="">-Type of Testing-</option>
				<option value="Usability Testing">Usability testing</option>
				<option value="Compatibility Testing">Compatibility Testing</option>
				<option value="Interface Testing">Interface Testing</option>
				<option value="Interface Testing">Services  Testing</option>
				<option value="Operational Testing">Operational  Testing</option>
				<option value="Installation Testing">Installation Testing</option>
				<option value="Security Testing">Security Testing</option>
			</select>

		</div>
	</div>
	<hr/>


<hr/>

<hr/>

<div class="row">

	<div class="col-lg-3 col-xs-3">
	</div>
	<div class="col-lg-2 col-xs-2">
		<label>Choose the OS</label>
	</div>
	<div class="col-lg-4 col-xs-4">

		<select name="platform" id="platform" ng-model="platform" required class="form-control">
			<option value="">-Operating System-</option>
			<option value="iOS">iOS</option>
			<option value="Android">Andriod OS</option>
			<option value="Blackberry">Blackberry</option>
			<option value="MeeGo">MeeGo</option>
			<option value="Palm">Palm</option>
			<option value="Windows">Windows</option>
		</select>

	</div>




	</div>
<hr/>


<hr/>

<div class="row">

		<div class="col-lg-4 col-xs-4">
		</div>
		<div class="col-lg-2 col-xs-2">
		</div>
		<div class="col-lg-2 col-xs-2">
			<input type="submit" class="btn btn-primary" value="CREATE" ng-click="addProject()"></input>
		</div>
		<div class="col-lg-4 col-xs-4">
		</div>
	</div>

	<div class="alert alert-success col-md-12" id="success-alert2" style="display: none ">
	<center> <strong> Successfully Added Project! Please check the list of projects</strong></center>
	</div>
		</form>
	</div>
</div>
</div>
</div>


